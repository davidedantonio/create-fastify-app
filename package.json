    "json-schema-ref-parser": "^7.1.3",
    "lodash": "^4.17.15",
    "make-promises-safe": "^5.1.0",
    "pino-colada": "^1.5.1",
    "pm2-axon": "^3.3.0",
    "pm2-axon-rpc": "^0.5.1",
